#include <iostream>
using namespace std;
int najelnie(int tab[], int a);
int main()
{
	
	int tab[5]{ 6, 21, 63, 25, 24 };
	int n=5;
	cout << najelnie(tab, n);
}
int najelnie(int tab[], int a) {
	int d=-1;
	for (int i = 0; i < a; i++) {
		if (tab[i] > d && tab[i] % 2 != 0)
			d = tab[i];
	}
	return d;
}

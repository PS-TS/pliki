  elseif (i<=7)
    i+10
  else
    i+100
    end;
end; clear;
kom = {}
kom{2} = 222
kom{3} = [1 2;3 4]
kom{:}
kom{3}(2,1)
x=[-pi:0.1:2*pi]
y= sin(x)
plot(y)
plot(x,y)
plot(x,y, '--r')
plot(x,y, '..k', 'LineWidth',2)
plot(x,y, 's')
hold on
plot(x, -y. 'r')
hold off
clc

xx=[-1;0.1;1];
yy=[-10:1:10];
for ix = 1:length(xx)
  x=xx(ix)
   for iy = 1:length(yy)
     y=yy(iy);
     mac(iy,ix) = sin(x*pi) .* sin(y/10);
   endfor
  end;
  surfl(mac)

